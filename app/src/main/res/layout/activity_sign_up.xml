<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".utils.SignUp">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/castoro"
        android:text="Register"
        android:textColor="#006D6E"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.example.qr_scanner.utils.CustomEditText
        android:id="@+id/textView14"
        android:layout_width="311dp"
        android:layout_height="45dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/roundshape"
        android:backgroundTint="#ADDAD9"
        android:hint="      Last Name"
        android:textAlignment="gravity"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <com.example.qr_scanner.utils.CustomEditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="316dp"
        android:layout_height="40dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/roundshape"
        android:backgroundTint="#ADDAD9"
        android:ems="10"
        android:hint="      E-mail"
        android:inputType="textEmailAddress"
        android:textAlignment="gravity"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <com.example.qr_scanner.utils.CustomEditText
        android:id="@+id/textView20"
        android:layout_width="319dp"
        android:layout_height="40dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/roundshape"
        android:backgroundTint="#ADDAD9"
        android:hint="      ID"
        android:textAlignment="gravity"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress" />


    <com.example.qr_scanner.utils.CustomEditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="319dp"
        android:layout_height="39dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/roundshape"
        android:backgroundTint="#ADDAD9"
        android:ems="10"
        android:hint="      Password"
        android:inputType="text"
        android:textAlignment="gravity"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etuser" />

    <com.example.qr_scanner.utils.CustomEditText
        android:id="@+id/etuser"
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/roundshape"
        android:backgroundTint="#ADDAD9"
        android:ems="10"
        android:hint="      User Name"
        android:inputType="text"
        android:textAlignment="gravity"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <com.example.qr_scanner.utils.CustomEditText
        android:id="@+id/editTextTextPassword3"
        android:layout_width="324dp"
        android:layout_height="40dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/roundshape"
        android:backgroundTint="#ADDAD9"
        android:ems="10"
        android:hint="      Re-enter Password"
        android:inputType="text"
        android:textAlignment="gravity"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2" />

    <Button
        android:id="@+id/log"
        android:layout_width="138dp"
        android:layout_height="55dp"
        android:layout_marginTop="36dp"
        android:backgroundTint="#006E6C"
        android:fontFamily="@font/sitka"
        android:text="Login"
        android:textSize="24sp"
        app:layout_anchorGravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.886"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword3" />

    <com.example.qr_scanner.utils.CustomEditText
        android:id="@+id/textView13"
        android:layout_width="312dp"
        android:layout_height="45dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/roundshape"
        android:backgroundTint="#ADDAD9"
        android:hint="      First Name"
        android:textAlignment="gravity"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

</androidx.constraintlayout.widget.ConstraintLayout>